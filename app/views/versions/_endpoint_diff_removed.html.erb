<div class="lines-container">
  <table class="lines">
    <thead>
    <tr>
      <th><%= endpoint_name %></th>
    </tr>
    </thead>
    <tbody>
    <% diff_lines.each do |diff_line| %>
      <tr>
        <td><%= diff_line.line_with_span_if_primitive %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
